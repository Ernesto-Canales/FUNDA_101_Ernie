Algoritmo calcular_dino
	Escribir 'Nombre del Dino:'
	Leer nombre
	Escribir 'Peso: (en lb)'
	Leer peso_lb
	Escribir 'Longitud: (en ft)'
	Leer long_ft
	peso_kg <- (peso_lb/2.205)
	long_m <- (long_ft/3.281)
	Escribir ''
	Escribir 'Dino: ',nombre
	Escribir 'Peso: ',peso_kg,' kg'
	Escribir 'Longitud: ',long_m,' m'
FinAlgoritmo
